{
  "extends": [
    "tslint-ionic-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "ban-comma-operator": false,
    "no-empty": [
      true,
      "allow-empty-catch"
    ],
    "no-floating-promises": false,
    "no-import-side-effect": false,
    "no-var-keyword": false,
    "no-var-requires": false,
    "prefer-conditional-expression": false
  },
  "rulesDirectory": [
    "tslint-eslint-rules",
    "tslint-plugin-prettier"
  ]
}
